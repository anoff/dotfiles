[user]
	name = Andreas Offenhaeuser
	email = offenhaeuser@gmail.com
[core]
	excludesfile = /Users/andreas/.gitignore_global
	editor = code --wait
[push]
	default = simple
[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[credential]
	helper = osxkeychain
[gui]
	editor = nano
[diff]
	tool = meld
[merge]
	tool = meld
